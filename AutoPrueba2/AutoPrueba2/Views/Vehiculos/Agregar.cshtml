<div id="appAutos">
        <div class="text-center">
            <h1 class="display-4">REGISTRO DE AUTOS</h1>
            <form method="post" action="/Vehiculos/Add" id="vehiculoform">
                <fieldset>
                    <div class="form">
                        <div class="col-md-4 mb-3">
                            <label>Tipo </label>
                            <input  v-model="auto.Tipo" @@keyup.enter="crearAuto" for="Tipo" type="text" name="Tipo" class="form-control" id="Tipo" />


                        </div>
                        <div class="col-md-4 mb-3">
                            <label>Marca </label>
                            <input v-model="auto.Marca" @@keyup.enter="crearAuto" type="text" name="Marca"  id="Marca" class="form-control"  />

                        </div>


                        <div class="col-md-4 mb-3">
                            <label>Modelo</label>
                            <input v-model="auto.Modelo" @@keyup.enter="crearAuto" type="text" name="Modelo" id="Modelo" class="form-control"/>

                        </div>
                        <div class="col-md-4 mb-3">
                            <label>Color</label>
                            <input v-model="auto.Color" @@keyup.enter="crearAuto" type="text" name="Color" id="Color" class="form-control" />

                        </div>
                        <div class="col-md-4 mb-3">
                            <label>Deposito</label>
                            <input v-model="auto.Deposito" @@keyup.enter="crearAuto" type="text" name="Deposito" id="Deposito" class="form-control" />

                        </div>

                        <div class="col-md-4 mb-3">
                            <label>MatriculaCaracterUno</label>
                            <input v-model="auto.MatriculaCaracterUno" @@keyup.enter="crearAuto" type="text" name="MatriculaCaracterUno" id="MatriculaCaracterUno" class="form-control" />

                        </div>


                        <div class="col-md-4 mb-3">
                            <label>MatriculaCaracterDos</label>
                            <input v-model="auto.MatriculaCaracterDos" @@keyup.enter="crearAuto"  type="number" name="MatriculaCaracterDos" id="MatriculaCaracterDos" class="form-control" />

                        </div>


                        <div class="col-md-4 mb-3">
                            <label>MatriculaCaracterTres</label>
                            <input v-model="auto.MatriculaCaracterTres" @@keyup.enter="crearAuto" type="text" name="MatriculaCaracterTres"  id="MatriculaCaracterTres" class="form-control" />

                        </div>

                        <div class="col-md-4 mb-3">
                            <label>Fecha</label>
                            <input v-model="auto.Fecha" @@keyup.enter="crearAuto" type="date" name="Fecha"  id="Fecha" class="form-control" />

                        </div>

                        <button id="submit" class="btn btn-primary" v-on:click="crearAuto">Enviar</button>
                    </div>
                </fieldset>

            </form>
        </div>
    </div>

@section ScriptsSection {
    <script src="~/js/Vehiculo-validator.js"></script>
    @*<script src="~/js/Lista.js"></script>*@
    <script src="https://ajax.google0
            apis.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.16.0/jquery.validate.min.js"></script>
    <script src="~/js/vue.js"></script>
    <script>
        $("#vehiculoform").validate();
    </script>

    <script src="~/js/Formulario.js"></script>
